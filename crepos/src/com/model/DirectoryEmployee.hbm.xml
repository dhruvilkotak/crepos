<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.model">

    <class name="DirectoryEmployee" table="directoryemployee">
         <id name="directoryEmployeeId" column="directoryEmployeeId">
            <generator class="increment"/>
        </id>
        <property name="employeeId" type="integer" column="employeeId"/>
         <property name="directoryId" type="integer" column="directoryId"/>
         <property name="ateManagerId" type="integer" column="ateManagerId"/>
         <property name="permission" type="string" column="permission"/>
    </class>

</hibernate-mapping>