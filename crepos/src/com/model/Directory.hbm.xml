<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.model">

    <class name="Directory" table="directory">
      <id name="directoryId" column="directoryId">
            
        </id>
        <property name="dirName" type="string" column="dirName"/>
        <property name="ownerId" type="integer" column="ownerId"/>
        <property name="permission" type="string" column="permission"/>
    </class>

</hibernate-mapping>